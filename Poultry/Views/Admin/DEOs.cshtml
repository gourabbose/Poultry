@model List<Poultry.Models.ViewModels.UserList>
@{
    ViewBag.Title = "DEOs";
}

<h2>Data Entry Operators</h2>

<p>
    @Html.ActionLink("Create New", "CreateUser")
</p>
@if(Model.Count()>0)
{
<table>
    <tr>
        <th>
            @Html.DisplayName("Username")
        </th>
        <th>
            @Html.DisplayName("Last login")
        </th>
        <th>
            @Html.DisplayName("Last pass Reset")
        </th>
        <th>Operations
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Membership.LastLoginDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Membership.LastPasswordChangedDate)
            </td>
            <td>
                @Html.ActionLink("Reset Password", "ResetPass", new { UserName = item.UserId, redirect = "DEO" }) |
            @*@Html.ActionLink("Delete", "Delete", new { id = item.UserId, redirect = "DEO" })*@
            </td>
        </tr>
    }
</table>
}
else
{
 <strong>No Data Found.<br /> Please Create "Data Entry Operator" Logins First.</strong>   
}
