@model IEnumerable<Poultry.Models.ConsumptionLog>
@if (Model.Count() > 0)
{
    foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Date.ToString("dd-MMM-yyyy")
            </td>
            <td>
                @item.Item.Name
            </td>
            <td>
                @item.Quantity @item.For.Unit
            </td>
            <td>
                @item.Ratio %
            </td>
            <td>
                @item.For.Name
            </td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="5" style="text-align: center"><strong>No Data Found</strong></td>
    </tr>   
}
